{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json}'",
    "test": "uvu -r solid-register"
  },
  "license": "MIT",
  "devDependencies": {
    "jsdom": "^20.0.3",
    "prettier": "^2.8.0",
    "solid-register": "^0.2.5",
    "solid-testing-library": "^0.5.0",
    "typescript": "^4.9.3",
    "uvu": "^0.5.6",
    "vite": "^3.2.4",
    "vite-plugin-solid": "^2.4.0"
  },
  "dependencies": {
    "@suid/material": "^0.8.0",
    "solid-app-router": "^0.4.2",
    "solid-js": "^1.6.2"
  },
  "solid-register": {
    "compile": {
      "solid": {
        "engine": "solid",
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
